{
  "name": "yjs",
  "version": "13.0.0-72",
  "description": "A ",
  "module": "./index.mjs'",
  "sideEffects": false,
  "scripts": {
    "test": "npm run lint",
    "build": "rm -rf build && rollup -c",
    "watch": "rollup -wc",
    "debug": "concurrently 'rollup -wc' 'cutest-serve build/y.test.js -o'",
    "lint": "standard **/*.js",
    "docs": "rm -rf docs; jsdoc --configure ./.jsdoc.json --verbose --readme ./README.md --package ./package.json || true",
    "serve-docs": "npm run docs && serve ./docs/",
    "postversion": "npm run build",
    "websocket-server": "node --experimental-modules ./provider/websocket/server.mjs"
  },
  "files": [
    "bindings/*",
    "docs/*",
    "examples/*",
    "lib/*",
    "persistences/*",
    "protocols/*",
    "provider/*",
    "bindings/*",
    "structs/*",
    "tests/*",
    "types/*",
    "utils/*",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "dictionaries": {
    "doc": "docs",
    "example": "examples",
    "test": "tests",
    "lib": "./"
  },
  "bin": {
    "y-websockets": "provider/websocket/server.js"
  },
  "standard": {
    "ignore": [
      "/build",
      "/node_modules",
      "/rollup.test.js",
      "/rollup.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/y-js/yjs.git"
  },
  "keywords": [
    "crdt"
  ],
  "author": "Kevin Jahns",
  "email": "kevin.jahns@rwth-aachen.de",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/yjs/issues"
  },
  "homepage": "http://y-js.org",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "concurrently": "^3.6.1",
    "cutest": "^0.1.9",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdoc": "^3.5.5",
    "prosemirror-example-setup": "^1.0.1",
    "prosemirror-schema-basic": "^1.0.0",
    "prosemirror-state": "^1.2.2",
    "prosemirror-view": "^1.6.5",
    "quill": "^1.3.6",
    "quill-cursors": "^1.0.3",
    "rollup": "^0.58.2",
    "rollup-cli": "^1.0.9",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-regenerator-runtime": "^6.23.1",
    "rollup-watch": "^3.2.2",
    "standard": "^11.0.1",
    "tui-jsdoc-template": "^1.2.2"
  },
  "dependencies": {
    "ws": "^6.1.0"
  }
}
